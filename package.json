{
  "name": "youtube-viewer",
  "version": "1.0.0",
  "description": "Set of utils to use puppeteer",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "docker:build": "docker-compose build --no-cache",
    "docker:start": "docker-compose up -d --scale",
    "docker:down": "docker-compose down",
    "start:prod": "NODE_ENV=production node viewer",
    "dev": "yarn build -- --watch",
    "watch": "yarn dev",
    "start:dev": "node viewer",
    "account": "nodemon account.ts",
    "start:searcher": "node searcher",
    "lint": "yarn lint:prettier .",
    "lint:prettier": "prettier --list-different --loglevel error",
    "lint:prettier:write": "yarn lint:prettier --write",
    "clean": "yarn clean:node && yarn clean:npm && yarn clean:yarn",
    "clean:node": "rm --force --recursive node_modules/ && mkdir node_modules",
    "clean:npm": "rm --force npm-*.log npm-shrinkwrap.json package-lock.json && npm cache clear --force",
    "clean:yarn": "rm --force yarn-*.har yarn-*.log yarn.lock && yarn cache clean",
    "node:inspect": "NODE_OPTIONS=\"--inspect=127.0.0.1:9229\" $($1)",
    "node:max-old-space-size:1GB": "NODE_OPTIONS=\"--max-old-space-size=1024\" $($1)",
    "node:max-old-space-size:2GB": "NODE_OPTIONS=\"--max-old-space-size=2048\" $($1)",
    "node:max-old-space-size:3GB": "NODE_OPTIONS=\"--max-old-space-size=3072\" $($1)",
    "node:max-old-space-size:4GB": "NODE_OPTIONS=\"--max-old-space-size=4096\" $($1)",
    "node:max-old-space-size:8GB": "NODE_OPTIONS=\"--max-old-space-size=8192\" $($1)",
    "node:max-old-space-size:16GB": "NODE_OPTIONS=\"--max-old-space-size=16384\" $($1)",
    "node:max-old-space-size:32GB": "NODE_OPTIONS=\"--max-old-space-size=32768\" $($1)",
    "node:max-old-space-size:64GB": "NODE_OPTIONS=\"--max-old-space-size=65536\" $($1)",
    "node:max-old-space-size:128GB": "NODE_OPTIONS=\"--max-old-space-size=131072\" $($1)",
    "node:max-old-space-size:256GB": "NODE_OPTIONS=\"--max-old-space-size=262144\" $($1)",
    "node:max-old-space-size:512GB": "NODE_OPTIONS=\"--max-old-space-size=524288\" $($1)",
    "node:max-old-space-size:1TB": "NODE_OPTIONS=\"--max-old-space-size=1048576\" $($1)",
    "reset": "yarn clean && yarn install",
    "silent": "$($1) > /dev/null 2>&1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ytviews/youtube-viewer.git"
  },
  "author": {
    "name": "Patrick LUZOLO",
    "email": "eldorplus@gmail.com",
    "url": "https://github.com/eldorplus"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ytviews/youtube-viewer/issues"
  },
  "dependencies": {
    "@cliqz/adblocker-puppeteer": "^1.20.4",
    "@httpie/json": "^1.1.0",
    "awesome-phonenumber": "^2.58.0",
    "axios": "^0.21.1",
    "chalk": "^4.0.0",
    "cheerio": "^1.0.0-rc.10",
    "co-body": "^6.1.0",
    "commander": "^8.2.0",
    "cross-fetch": "^3.1.2",
    "getsmscode": "^1.0.4",
    "koa": "^2.13.1",
    "koa-router": "^10.1.1",
    "lodash": "^4.17.20",
    "ngrok": "^4.1.0",
    "node-schedule": "^2.0.0",
    "nodemon": "^2.0.12",
    "p-event": "^4.2.0",
    "p-memoize": "^4.0.1",
    "p-retry": "^4.6.1",
    "p-timeout": "^5.0.0",
    "parse-otp-message": "^1.0.2",
    "parsetime": "^0.2.9",
    "pify": "^5.0.0",
    "plivo": "^4.22.0",
    "puppeteer": "^10.1.0",
    "puppeteer-extra": "^3.1.9",
    "puppeteer-extra-plugin-adblocker": "^2.11.11",
    "puppeteer-extra-plugin-anonymize-ua": "^2.2.17",
    "puppeteer-extra-plugin-stealth": "^2.4.9",
    "puppeteer-extra-plugin-user-data-dir": "^2.2.12",
    "puppeteer-extra-plugin-user-preferences": "^2.2.12",
    "request-promise-native": "^1.0.9",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "user-agents": "^1.0.751",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@httpie/tsconfig": "^3.1.0",
    "@types/co-body": "^6.1.0",
    "@types/koa-router": "^7.4.4",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.7.1",
    "@types/node-schedule": "^1.3.2",
    "@types/pify": "^5.0.1",
    "@prettier/plugin-ruby": "1.6.1",
    "@prettier/plugin-xml": "1.0.2",
    "@types/prettier": "2.3.2",
    "@types/puppeteer": "^5.4.4",
    "@types/request-promise-native": "^1.0.18",
    "@types/user-agents": "^1.0.2",
    "@types/uuid": "^8.3.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "*",
    "prettier": "2.3.2"
  },
  "keywords": [
    "youtube",
    "viewer"
  ],
  "publishConfig": {
    "access": "public"
  }
}
